<settings>
  <font>Tahoma</font>
  <hide_bullets_count>18</hide_bullets_count>
  <data_type>csv</data_type>
  <reload_data_interval>20</reload_data_interval>
  <add_time_stamp>1</add_time_stamp>
  <background>
    <alpha>90</alpha>
    <border_alpha>10</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>50</left>
      <right>40</right>
      <bottom>65</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <approx_count>9</approx_count>
    </x>
    <y_left>
      <alpha>10</alpha>
    </y_left>
  </grid>
  <axes>
    <x>
      <width>1</width>
      <color>0D8ECF</color>
    </x>
    <y_left>
      <width>1</width>
      <color>0D8ECF</color>
    </y_left>
  </axes>
  <values>
    <x>
      <rotate>45</rotate>
    </x>
    <y_left>
      <min>0</min>
      <max>100</max>
      <unit>%</unit>
    </y_left>
  </values>
  <indicator>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <balloon>
    <on_off>0</on_off>
  </balloon>
  <legend>
    <graph_on_off>0</graph_on_off>
    <align>center</align>
  </legend>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
  <help>
    <button>
      <color>FCD202</color>
      <text_color>000000</text_color>
      <text_color_hover>FF0F00</text_color_hover>
    </button>
    <balloon>
      <text><![CDATA[Click on the graph to turn on/off value baloon <br/><br/>Click on legend key to show/hide graph<br/><br/>Mark the area you wish to enlarge]]></text>
      <color>FCD202</color>
      <text_color>000000</text_color>
    </balloon>
  </help>
  <graphs>
    <graph gid="0">
      <title>Load</title>
      <color>990000</color>
      <color_hover>FF0F00</color_hover>
      <line_width>3</line_width>
      <selected>0</selected>
    </graph>
    <graph gid="1">
      <title>Temperature</title>
      <color>B0DE09</color>
      <color_hover>FF0F00</color_hover>
      <line_width>3</line_width>
      <balloon_text><![CDATA[<b>{value} Â°C</b><br>{description}]]></balloon_text>
      <bullet>round</bullet>
    </graph>
    <graph gid="2">
      <title>Fanspeed</title>
      <color>0066FF</color>
      <line_width>3</line_width>
      <balloon_text><![CDATA[<b>{value} %</b><br>{description}]]></balloon_text>
    </graph>
    <graph gid="3">
<visible_in_legend>0</visible_in_legend>
      <hidden>1</hidden>
      <selected>0</selected>
    </graph>
 
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b>System stats of my miner</b>]]></text>
      <y>25</y>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>
